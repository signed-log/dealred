{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "1.0",
  "description": "__MSG_extensionDescription__",
  "default_locale": "fr",
  "icons": {
    "18": "assets/icons/icon_18dp.png",
    "24": "assets/icons/icon_24dp.png",
    "36": "assets/icons/icon_36dp.png",
    "48": "assets/icons/icon_48dp.png",
    "72": "assets/icons/icon_36x2dp.png",
    "96": "assets/icons/icon_48x2dp.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://dealabs.digidip.net/*",
    "https://www.smartredirect.de/*"
  ],
  "background": {
    "scripts": ["browser-polyfill.js", "dealabs.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "69.0"
    }
  }
}
